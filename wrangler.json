{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "gnosis",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-14",
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "memory-index"
    }
  ],
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "OPENAI_API_KEY": "ensure-this-is-set"
  },
  "ai": {
    "binding": "AI"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}
