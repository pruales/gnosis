# Gnosis API Configuration
# Copy this file to .env and fill in your values

# API key for authentication (required)
GNOSIS_API_KEY=your_api_key_here

# Base URL of the API (optional, defaults to http://localhost:8787)
GNOSIS_API_URL=http://localhost:8787

# User ID for testing memories (optional, defaults to test-user-id)
TEST_USER_ID=test-user-id 